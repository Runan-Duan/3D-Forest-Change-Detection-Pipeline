{
  "pipeline": [
    {
      "type": "readers.las",
      "filename": "${INPUT}"
    },
    {
      "type": "filters.icp",
      "target": "${OUTPUT}",
      "max_iterations": 100,
      "max_error": 0.1,
      "transformation": "rigid",
      "convergence": 1e-7,
      "use_point_to_plane": true
    },
    {
      "type": "writers.las",
      "filename": "${OUTPUT}"
    }
  ]
}
